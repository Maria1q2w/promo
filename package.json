{
  "name": "promo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "prettier": "prettier --ignore-unknown --check .",
    "prettier:fix": "prettier --ignore-unknown --write .",
    "types": "vue-tsc --noEmit",
    "eslint": "eslint --quiet --ext .js,.mjs,.ts,.tsx . --color  --ignore-path .gitignore",
    "eslint:fix": "eslint --quiet --ext .js,.mjs,.ts,.tsx . --color --fix  --ignore-path .gitignore",
    "stylelint": "stylelint **/*.{scss,css} --quiet  --ignore-path .gitignore",
    "stylelint:fix": "stylelint **/*.{scss,css} --quiet --fix --ignore-path .gitignore",
    "quality": "run-p --aggregate-output prettier eslint stylelint types",
    "format": "run-s prettier:fix eslint:fix stylelint:fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vee-validate/rules": "^4.15.1",
    "@vuepic/vue-datepicker": "^11.0.2",
    "@vueuse/core": "13.5.0",
    "vee-validate": "^4.15.1",
    "vue": "3.5.17",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@typescript-eslint/eslint-plugin": "7.16.1",
    "@typescript-eslint/parser": "7.16.1",
    "@vitejs/plugin-vue": "5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "@vue/compiler-core": "3.5.17",
    "basic-ftp": "5.0.5",
    "cli-progress": "3.12.0",
    "dotenv": "17.2.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-destructure-depth": "1.0.3",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-typescript-sort-keys": "3.2.0",
    "eslint-plugin-vue": "9.24.0",
    "lint-staged": "16.1.2",
    "npm-run-all2": "8.0.4",
    "postcss": "8.5.6",
    "postcss-scss": "4.0.9",
    "prettier": "3.6.2",
    "sass": "1.89.2",
    "stylelint": "16.21.1",
    "stylelint-config-sass-guidelines": "12.1.0",
    "stylelint-order": "7.0.0",
    "stylelint-prettier": "5.0.3",
    "typescript": "5.8.3",
    "vite": "6.3.4",
    "vite-plugin-checker": "0.9.3",
    "vite-plugin-html": "3.2.2",
    "vite-svg-loader": "^5.1.0",
    "vue-eslint-parser": "9.4.3",
    "vue-tsc": "2.2.12"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all",
      "Safari >= 15",
      "Firefox >= 67",
      "Chrome >= 86"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{tsx,ts,js,mjs}": "eslint --fix",
    "*.{css,scss}": "stylelint --fix"
  }
}
